    "version": "0.2.0",
    "configurations": [
        {
            "name": "cmake c++",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/profitview_socketio_cpp",
            "args": [
                "223834ffcc42d292c3a5fdc156c244ad2b020465",
                "trade:bitmex:XBTUSD", 
                "trade:bitmex:ETHUSD",
                "trade:bitmex:XRPUSD"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "CMake build project",
            "miDebuggerPath": "/usr/bin/gdb",
        }
    ]
}